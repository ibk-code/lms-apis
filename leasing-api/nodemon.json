{
  "restartable": "rs",
  "ignore": [".git", "node_modules/**/node_modules"],
  "verbose": true,
  "execMap": {
    "js": "node --harmony"
  },
  "events": {
    "restart": ""
  },
  "watch": ["src/*", "bin/*", "prettier.config.js", "eslintrc.js", "config/*"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js,json"
}
